<head>
  <meta charset="UTF-8">
  <title>Student Management System</title>
    
  <!---CDN Links-->
  <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
  <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css">
  <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.1/js/bootstrap.min.js"></script>  
  <style type="text/css" rel="stylesheet">
      input{
        height: 40px !important;
      }
  </style>
  </head>
<body>
  <div class="page-wrapper">
    <div class="row">
      <nav class="navbar navbar-inverse" role="navigation" style="padding-left:130px;">
           <ul class="nav navbar-nav">
              <li class="active"><a href="/">Student Record Management</a></li>
           </ul>
      </nav>
    </div>
    <div class="row jumbotron">
      <div class="col-md-4 col-md-offset-4">
          <h3 class="text-info">Student Information</h3>
          <div class="row">
            <div class="col-md-6"><h4 class="text-success">Surname</h4></div>
            <div class="col-md-6"><h4 class="text-success"><%=data[0].sname%></h4></div>
          </div>
          <div class="row">
            <div class="col-md-6"><h4 class="text-success">First Name</h4></div>
            <div class="col-md-6"><h4 class="text-success"><%=data[0].fname%></h4></div>
          </div>
          <div class="row">
            <div class="col-md-6"><h4 class="text-success">Email</h4></div>
            <div class="col-md-6"><h4 class="text-success"><%=data[0].email%></h4></div>
          </div>
          <div class="row">
            <div class="col-md-6"><h4 class="text-success">Phone Number</h4></div>
            <div class="col-md-6"><h4 class="text-success"><%=data[0].phoneno%></h4></div>
          </div>
          <div class="row">
            <div class="col-md-6"><h4 class="text-success">Gender</h4></div>
            <div class="col-md-6"><h4 class="text-success"><%=data[0].gender%></h4></div>
          </div>
          <div class="row">
            <div class="col-md-6"><h4 class="text-success">Address</h4></div>
            <div class="col-md-6"><h4 class="text-success"><%=data[0].address%></h4></div>
          </div>
      </div>
    </div>
  </div>

  <script type="text/javascript">
      
  </script>
</body>
</html>